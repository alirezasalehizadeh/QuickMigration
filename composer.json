{
    "name": "alirezasalehizadeh/quick-migration",
    "description": "A quick package for implement migrations in your PHP project.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alirezasalehizadeh\\QuickMigration\\": "src/"
        }
    },
    "autoload-dev": {
